            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/RegisterUserResponseDto"
                }
              }
            }
          },
          "400": {
            "description": "Bad Request",
            "content": {
              "application/json": {
                "schema": {
                  "oneOf": [
                    {
                      "$ref": "#/components/schemas/ValidationError"
                    },
                    {
                      "type": "object",
                      "required": [
                        "statusCode",
                        "message",
                        "error"
                      ],
                      "properties": {
                        "statusCode": {
                          "type": "number",
                          "example": 400
                        },
                        "message": {
                          "type": "string",
                          "example": "Error while creating user"
                        },
                        "error": {
                          "type": "string",
                          "example": "Bad Request"
                        }
                      }
                    }
                  ]
                }
              }
            }
          },
          "503": {
            "description": ""
          }
        },
        "summary": "User registration",
        "tags": [
          "Auth"
        ]
      }
    },
